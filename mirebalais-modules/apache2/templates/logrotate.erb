/var/log/apache2/ssl_access.log  {
  daily
  rotate 7
  size 10M
  copytruncate
  compress
  notifempty
  missingok
  sharedscripts
  #prerotate
  #/usr/local/cgi-bin/awstats.pl -config=<%= @site_domain %> -update > /dev/null
  endscript 
}

/var/log/apache2/*.log {
  daily
  rotate 7
  notifempty
  missingok
}

